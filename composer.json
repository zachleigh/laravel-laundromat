{
    "name": "zachleigh/laravel-laundromat",
    "description": "Clean up objects before sending them client-side.",
    "keywords": ["laravel"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Zach Leigh",
            "email": "zachleigh@fastmail.jp"
        }
    ],

    "require": {
        "php": ">=5.5.9",
        "laravel/browser-kit-testing": "^1.0"
    },

    "require-dev": {
        "laravel/laravel": "5.4.*",
        "phpunit/phpunit": "~4.0"
    },

    "autoload": {
        "psr-4": {
            "LaravelLaundromat\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "LaravelLaundromat\\tests\\": "tests/"
        }
    },

    "minimum-stability": "stable",
    "prefer-stable": true
}
